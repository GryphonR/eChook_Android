<merge xmlns:android="http://schemas.android.com/apk/res/android">
	<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
					xmlns:tools="http://schemas.android.com/tools"
					xmlns:custom="http://schemas.android.com/apk/res-auto"
					android:layout_width="match_parent"
					android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
					android:paddingRight="@dimen/activity_horizontal_margin"
					android:paddingTop="@dimen/activity_vertical_margin"
					android:paddingBottom="@dimen/activity_vertical_margin"
					tools:context="com.driven.rowan.drivenbluetooth.MainActivity"
					android:id="@+id/main">

		<TextView android:text="Label" android:layout_width="wrap_content"
				  android:layout_height="wrap_content"
				  android:id="@+id/label"
				  android:editable="false"
				  android:layout_alignParentTop="true"
				  android:layout_alignParentLeft="true"
				  android:layout_alignParentStart="true"/>

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Open BT"
			android:id="@+id/open"
			android:layout_alignParentBottom="true"
			android:layout_alignParentStart="true"
			android:onClick="OpenBT"/>

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Close BT"
			android:id="@+id/close"
			android:layout_alignParentBottom="true"
			android:layout_toRightOf="@+id/open"
			android:layout_toEndOf="@+id/open"
			android:onClick="CloseBT"/>

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Settings"
			android:id="@+id/btnSettings"
			android:onClick="LaunchSettings"
			android:layout_alignParentBottom="true"
			android:layout_toEndOf="@+id/close"/>

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Start"
			android:id="@+id/start"
			android:textStyle="bold"
			android:layout_alignParentBottom="true"
			android:layout_toLeftOf="@+id/stop"
			android:layout_toStartOf="@+id/stop"
			android:onClick="StartAllThreads"/>

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Stop"
			android:id="@+id/stop"
			android:layout_alignParentBottom="true"
			android:layout_alignParentRight="true"
			android:layout_alignParentEnd="true"
			android:onClick="CancelAllThreads"/>

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Graph"
			android:id="@+id/graph"
			android:layout_alignParentBottom="true"
			android:layout_toLeftOf="@+id/start"
			android:onClick="ViewGraph"
			android:layout_toEndOf="@+id/btnSettings"/>





		<!-- **************************	-->
		<!--		 THROTTLE			-->
		<!-- **************************	-->

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:text="Throttle"
			android:id="@+id/textView"
			android:gravity="center"
			android:editable="false"
			android:layout_marginTop="25dp"
			android:layout_below="@+id/label"
			android:layout_alignParentLeft="true"
			android:layout_alignParentStart="true"
			android:layout_toLeftOf="@+id/textView2"
			android:layout_toStartOf="@+id/textView2"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:id="@+id/throttle"
			android:inputType="none"
			android:width="90dp"
			android:gravity="center"
			android:editable="false"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="false"
			android:layout_below="@+id/textView"
			android:textSize="20dp"
			android:textStyle="bold"
			/>

		<com.driven.rowan.drivenbluetooth.DataBar
			android:layout_width="wrap_content"
			android:layout_height="100dp"
			android:layout_alignParentLeft="true"
			android:layout_alignParentStart="true"
			android:id="@+id/ThrottleBar"
			android:layout_alignRight="@+id/throttle"
			android:layout_below="@+id/current"
			custom:BarColor="#000000"
			custom:min="0"
			custom:max="100"
			custom:xWidth="@integer/databar_xWidth"
			/>

		<!-- **************************	-->
		<!--		 VOLTAGE			-->
		<!-- **************************	-->

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:text="Voltage"
			android:gravity="center"
			android:id="@+id/textView2"
			android:layout_alignTop="@+id/textView"
			android:layout_alignLeft="@+id/voltage"
			android:layout_alignRight="@+id/voltage"
			android:layout_alignBottom="@+id/textView"
			android:layout_alignEnd="@+id/voltage"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:id="@+id/voltage"
			android:gravity="center"
			android:singleLine="false"
			android:inputType="none"
			android:width="90dp"
			android:layout_alignTop="@+id/throttle"
			android:layout_toRightOf="@+id/throttle"
			android:textSize="20dp"
			android:layout_alignBottom="@+id/throttle"
			android:textStyle="bold"
			/>

		<com.driven.rowan.drivenbluetooth.DataBar
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.driven.rowan.drivenbluetooth.DataBar"
			android:id="@+id/VoltageBar"
			android:layout_alignLeft="@+id/voltage"
			android:layout_alignBottom="@+id/ThrottleBar"
			android:layout_alignRight="@+id/voltage"
			android:layout_alignParentStart="false"
			android:layout_alignParentLeft="false"
			android:layout_alignTop="@+id/ThrottleBar"
			custom:BarColor="#A3C2FF"
			custom:min="0"
			custom:max="28"
			custom:xWidth="@integer/databar_xWidth"/>

		<!-- **************************	-->
		<!--		 CURRENT			-->
		<!-- **************************	-->
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:text="Current"
			android:gravity="center"
			android:id="@+id/textView3"
			android:layout_alignTop="@+id/textView2"
			android:layout_alignRight="@+id/current"
			android:layout_alignEnd="@+id/current"
			android:layout_alignLeft="@+id/current"
			android:layout_alignBottom="@+id/textView2"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:id="@+id/current"
			android:gravity="center"
			android:singleLine="false"
			android:inputType="none"
			android:width="90dp"
			android:layout_alignTop="@+id/voltage"
			android:layout_toRightOf="@+id/voltage"
			android:textSize="20dp"
			android:layout_alignBottom="@+id/voltage"
			android:textStyle="bold"
			/>

		<com.driven.rowan.drivenbluetooth.DataBar
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.driven.rowan.drivenbluetooth.DataBar"
			android:id="@+id/CurrentBar"
			android:layout_alignLeft="@+id/current"
			android:layout_alignBottom="@+id/ThrottleBar"
			android:layout_alignRight="@+id/current"
			android:layout_alignParentStart="false"
			android:layout_alignParentLeft="false"
			android:layout_alignTop="@+id/ThrottleBar"
			custom:BarColor="#ffffea2e"
			custom:min="0"
			custom:max="50"
			custom:xWidth="@integer/databar_xWidth"/>

		<!-- **************************	-->
		<!--		 TEMP 1				-->
		<!-- **************************	-->
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:text="Temp 1"
			android:gravity="center"
			android:id="@+id/textView4"
			android:layout_alignBottom="@+id/textView3"
			android:layout_alignRight="@+id/temp1"
			android:layout_alignEnd="@+id/temp1"
			android:layout_alignLeft="@+id/temp1"
			android:layout_alignTop="@+id/textView3"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:id="@+id/temp1"
			android:gravity="center"
			android:singleLine="false"
			android:inputType="none"
			android:width="90dp"
			android:layout_alignTop="@+id/current"
			android:layout_toRightOf="@+id/current"
			android:textSize="20dp"
			android:layout_marginLeft="30dp"
			android:layout_above="@+id/T1Bar"
			android:textStyle="bold"
			/>

		<com.driven.rowan.drivenbluetooth.DataBar
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.driven.rowan.drivenbluetooth.DataBar"
			android:id="@+id/T1Bar"
			android:layout_alignLeft="@+id/temp1"
			android:layout_alignBottom="@+id/ThrottleBar"
			android:layout_alignRight="@+id/temp1"
			android:layout_alignParentStart="false"
			android:layout_alignParentLeft="false"
			android:layout_alignTop="@+id/ThrottleBar"
			custom:BarColor="#ffff5358"
			custom:min="0"
			custom:max="85"
			custom:xWidth="@integer/databar_xWidth"/>

		<!-- **************************	-->
		<!--		 RPM				-->
		<!-- **************************	-->
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:id="@+id/rpm"
			android:gravity="center"
			android:singleLine="false"
			android:inputType="none"
			android:width="90dp"
			android:layout_toRightOf="@+id/temp1"
			android:layout_alignTop="@+id/temp1"
			android:textSize="20dp"
			android:layout_above="@+id/RPMBar"
			android:textStyle="bold"
			/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:text="RPM"
			android:gravity="center"
			android:id="@+id/textView5"
			android:layout_alignTop="@+id/textView4"
			android:layout_alignLeft="@+id/rpm"
			android:layout_alignRight="@+id/rpm"
			android:layout_alignEnd="@+id/rpm"
			android:layout_alignBottom="@+id/textView4"/>

		<com.driven.rowan.drivenbluetooth.DataBar
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.driven.rowan.drivenbluetooth.DataBar"
			android:id="@+id/RPMBar"
			android:layout_alignLeft="@+id/rpm"
			android:layout_alignBottom="@+id/ThrottleBar"
			android:layout_alignRight="@+id/rpm"
			android:layout_alignParentStart="false"
			android:layout_alignParentLeft="false"
			android:layout_alignTop="@+id/ThrottleBar"
			custom:BarColor="#ffff5358"
			custom:min="0"
			custom:max="2500"
			custom:xWidth="@integer/databar_xWidth"/>

		<!-- **************************	-->
		<!--		 SPEED				-->
		<!-- **************************	-->
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:text="Speed"
			android:gravity="center"
			android:id="@+id/textView7"
			android:layout_alignParentStart="false"
			android:layout_toRightOf="@+id/textView5"
			android:layout_alignBottom="@+id/textView5"
			android:layout_alignTop="@+id/textView5"
			android:layout_alignEnd="@+id/speed"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="30dp"
			android:id="@+id/speed"
			android:gravity="center"
			android:singleLine="false"
			android:inputType="none"
			android:width="90dp"
			android:layout_toRightOf="@+id/rpm"
			android:layout_alignTop="@+id/rpm"
			android:textSize="20dp"
			android:layout_above="@+id/SpeedBar"
			android:textStyle="bold"
			/>

		<com.driven.rowan.drivenbluetooth.DataBar
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.driven.rowan.drivenbluetooth.DataBar"
			android:id="@+id/SpeedBar"
			android:layout_alignLeft="@+id/speed"
			android:layout_alignBottom="@+id/ThrottleBar"
			android:layout_alignRight="@+id/speed"
			android:layout_alignParentStart="false"
			android:layout_alignParentLeft="false"
			android:layout_alignTop="@+id/ThrottleBar"
			custom:BarColor="#ffff5358"
			custom:min="0"
			custom:max="50"
			custom:xWidth="@integer/databar_xWidth"/>

		<!-- **************************	-->
		<!--		 something				-->
		<!-- **************************	-->

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txt_Mode"
			android:layout_alignParentRight="true"
			android:layout_alignParentEnd="true"
			android:layout_alignParentTop="true"/>

		<EditText
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:inputType="time"
			android:ems="10"
			android:id="@+id/raceStartTime"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@+id/textView4"
			android:layout_toLeftOf="@+id/textView7"
			android:layout_toStartOf="@+id/textView7"
			android:gravity="center"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="Race Start"
			android:id="@+id/textView6"
			android:layout_alignBottom="@+id/raceStartTime"
			android:layout_alignLeft="@+id/textView4"
			android:layout_alignStart="@+id/textView4"
			android:layout_toLeftOf="@+id/raceStartTime"
			android:layout_toStartOf="@+id/raceStartTime"
			android:layout_alignParentTop="true"
			android:gravity="center_vertical|center"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="Latitude     "
			android:id="@+id/textView10"
			android:layout_above="@+id/start"
			android:layout_alignStart="@+id/T1Bar"
			android:layout_alignEnd="@+id/T1Bar"
			android:gravity="right"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="Longitude     "
			android:id="@+id/textView9"
			android:layout_above="@+id/textView10"
			android:layout_alignStart="@+id/T1Bar"
			android:layout_alignEnd="@+id/T1Bar"
			android:gravity="right"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txtLongitude"
			android:layout_alignTop="@+id/textView9"
			android:layout_alignEnd="@+id/RPMBar"
			android:layout_alignStart="@+id/RPMBar"
			android:gravity="right"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txtLatitude"
			android:layout_below="@+id/txtLongitude"
			android:layout_alignEnd="@+id/start"
			android:layout_alignStart="@+id/start"
			android:gravity="right"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="Gx"
			android:id="@+id/textView8"
			android:layout_below="@+id/ThrottleBar"
			android:layout_alignParentStart="true"
			android:layout_alignRight="@+id/ThrottleBar"
			android:gravity="center"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="Gy"
			android:id="@+id/textView11"
			android:layout_alignTop="@+id/textView8"
			android:layout_alignStart="@+id/VoltageBar"
			android:gravity="center"
			android:layout_alignRight="@+id/VoltageBar"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="Gz"
			android:id="@+id/textView12"
			android:gravity="center"
			android:layout_alignTop="@+id/textView11"
			android:layout_toEndOf="@+id/textView11"
			android:layout_alignRight="@+id/CurrentBar"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txtGx"
			android:gravity="center"
			android:layout_below="@+id/textView8"
			android:layout_alignParentStart="true"
			android:layout_alignEnd="@+id/textView8"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txtGy"
			android:gravity="center"
			android:layout_below="@+id/textView11"
			android:layout_alignStart="@+id/textView11"
			android:layout_alignEnd="@+id/textView11"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txtGz"
			android:gravity="center"
			android:layout_below="@+id/textView12"
			android:layout_alignStart="@+id/textView12"
			android:layout_alignEnd="@+id/textView12"
			android:visibility="gone"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:text="arduino.csv"
			android:id="@+id/textView13"
			android:layout_below="@+id/label"
			android:layout_alignParentStart="true"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:id="@+id/txtDataFileSize"
			android:layout_alignBottom="@+id/textView13"
			android:layout_alignStart="@+id/textView2"/>

		<com.jjoe64.graphview.GraphView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/throttleGraph"
			android:layout_below="@+id/ThrottleBar"
			android:layout_alignParentStart="true"
			android:layout_toStartOf="@+id/VoltageBar"
			android:layout_above="@+id/open"/>

		<view
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.jjoe64.graphview.GraphView"
			android:id="@+id/voltsGraph"
			android:layout_below="@+id/VoltageBar"
			android:layout_alignStart="@+id/VoltageBar"
			android:layout_above="@+id/close"
			android:layout_toStartOf="@+id/btnSettings"
			android:layout_alignEnd="@+id/VoltageBar"/>

		<view
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			class="com.jjoe64.graphview.GraphView"
			android:id="@+id/ampsGraph"
			android:layout_below="@+id/CurrentBar"
			android:layout_alignStart="@+id/CurrentBar"
			android:layout_alignBottom="@+id/voltsGraph"
			android:layout_toStartOf="@+id/graph"/>

	</RelativeLayout>

	<FrameLayout android:id="@+id/graphview_overlay"
				 android:layout_width="match_parent"
				 android:layout_height="match_parent"
				 android:visibility="gone">

	</FrameLayout>
</merge>
