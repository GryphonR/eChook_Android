<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
				android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
				android:paddingRight="@dimen/activity_horizontal_margin"
				android:paddingTop="@dimen/activity_vertical_margin"
				android:paddingBottom="@dimen/activity_vertical_margin"
				tools:context="com.driven.rowan.drivenbluetooth.MainActivity"
				android:id="@+id/main">

	<TextView android:text="Label" android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:id="@+id/label"
			  android:editable="false"
			  android:layout_alignParentTop="true"
			  android:layout_alignParentLeft="true"
			  android:layout_alignParentStart="true"
			  android:layout_marginTop="27dp"/>

	<Button
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Open BT"
		android:id="@+id/open"
		android:layout_above="@+id/start"
		android:layout_alignParentLeft="true"
		android:layout_alignParentStart="true"/>

	<Button
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Start"
		android:id="@+id/start"
		android:layout_alignTop="@+id/stop"
		android:layout_centerHorizontal="true" />

	<Button
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Stop"
		android:id="@+id/stop"
		android:layout_alignParentBottom="true"
		android:layout_alignParentRight="true"
		android:layout_alignParentEnd="true"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Speed"
		android:id="@+id/textView"
		android:layout_marginBottom="100dp"
		android:layout_above="@+id/open"
		android:layout_alignParentLeft="true"
		android:layout_alignParentStart="true"
		android:layout_alignRight="@+id/speed"
		android:layout_alignEnd="@+id/speed"
		android:gravity="center"
		android:editable="false"/>

	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/speed"
		android:inputType="none"
		android:width="80dp"
		android:gravity="center"
		android:layout_centerVertical="true"
		android:layout_alignRight="@+id/open"
		android:layout_alignEnd="@+id/open"
		android:editable="false"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Voltage"
		android:gravity="center"
		android:id="@+id/textView2"
		android:layout_alignTop="@+id/textView"
		android:layout_alignLeft="@+id/voltage"
		android:layout_alignStart="@+id/voltage"
		android:layout_toLeftOf="@+id/textView3"
		android:layout_toStartOf="@+id/textView3"/>

	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/voltage"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="80dp"
		android:layout_alignTop="@+id/speed"
		android:layout_toRightOf="@+id/speed"
		android:layout_toEndOf="@+id/speed"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Current"
		android:gravity="center"
		android:id="@+id/textView3"
		android:layout_alignTop="@+id/textView2"
		android:layout_toRightOf="@+id/voltage"
		android:layout_alignRight="@+id/current"
		android:layout_alignEnd="@+id/current"/>

	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/current"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="80dp"
		android:layout_alignTop="@+id/voltage"
		android:layout_toRightOf="@+id/voltage"
		android:layout_toEndOf="@+id/voltage"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:id="@+id/SpeedDataCount"
		android:layout_alignTop="@+id/VoltsDataCount"
		android:layout_alignParentLeft="true"
		android:layout_alignParentStart="true"
		android:layout_alignRight="@+id/speed"
		android:layout_alignEnd="@+id/speed"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:id="@+id/VoltsDataCount"
		android:layout_alignTop="@+id/AmpsDataCount"
		android:layout_alignLeft="@+id/voltage"
		android:layout_alignStart="@+id/voltage"
		android:layout_toLeftOf="@+id/AmpsDataCount"
		android:layout_toStartOf="@+id/AmpsDataCount"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:id="@+id/AmpsDataCount"
		android:layout_below="@+id/current"
		android:layout_alignLeft="@+id/textView3"
		android:layout_alignStart="@+id/textView3"
		android:layout_alignRight="@+id/current"
		android:layout_alignEnd="@+id/current"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="@string/app_name"
		android:id="@+id/TitleTextView"
		android:layout_alignParentTop="true"
		android:layout_alignParentLeft="true"
		android:layout_alignParentStart="true"/>

	<Button
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Close BT"
		android:id="@+id/close"
		android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true"
		android:layout_alignParentStart="true"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Temp 1"
		android:gravity="center"
		android:id="@+id/textView4"
		android:layout_alignBottom="@+id/textView3"
		android:layout_toRightOf="@+id/textView3"
		android:layout_alignRight="@+id/temp1"
		android:layout_alignEnd="@+id/temp1"/>

	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/temp1"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="80dp"
		android:layout_alignTop="@+id/current"
		android:layout_toRightOf="@+id/current"
		android:layout_toEndOf="@+id/current"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:id="@+id/Temp1DataCount"
		android:layout_below="@+id/temp1"
		android:layout_toRightOf="@+id/current"
		android:layout_alignRight="@+id/temp1"
		android:layout_alignEnd="@+id/temp1"/>

	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/temp2"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="80dp"
		android:layout_alignBottom="@+id/temp1"
		android:layout_toRightOf="@+id/temp1"
		android:layout_toEndOf="@+id/temp1"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Temp 2"
		android:gravity="center"
		android:id="@+id/textView5"
		android:layout_alignTop="@+id/textView4"
		android:layout_alignLeft="@+id/temp2"
		android:layout_alignStart="@+id/temp2"
		android:layout_alignRight="@+id/temp2"
		android:layout_alignEnd="@+id/temp2"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:id="@+id/Temp2DataCount"
		android:layout_alignTop="@+id/Temp1DataCount"
		android:layout_alignLeft="@+id/temp2"
		android:layout_alignStart="@+id/temp2"
		android:layout_alignRight="@+id/temp2"
		android:layout_alignEnd="@+id/temp2"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Temp 2"
		android:gravity="center"
		android:id="@+id/textView7"
		android:layout_alignBottom="@+id/textView5"
		android:layout_toRightOf="@+id/textView5"
		android:layout_toStartOf="@+id/stop"
		android:layout_toLeftOf="@+id/stop"/>

	<EditText
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/temp3"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="80dp"
		android:layout_above="@+id/Temp2DataCount"
		android:layout_alignLeft="@+id/textView7"
		android:layout_alignStart="@+id/textView7"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:id="@+id/Temp3DataCount"
		android:layout_alignTop="@+id/Temp2DataCount"
		android:layout_alignLeft="@+id/temp3"
		android:layout_alignStart="@+id/temp3"
		android:layout_alignRight="@+id/temp3"
		android:layout_alignEnd="@+id/temp3"/>

</RelativeLayout>
