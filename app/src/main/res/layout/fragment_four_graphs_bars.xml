<RelativeLayout
    android:id="@+id/CenterView"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_above="@+id/open"
    android:layout_alignEnd="@+id/txt_Mode"
    android:layout_below="@+id/textView8"
    android:gravity="center">

	<view
        android:id="@+id/voltsGraph"
        class="com.github.mikephil.charting.charts.LineChart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ampHours"
        android:layout_alignEnd="@+id/VoltageBar"
        android:layout_alignLeft="@+id/VoltageBar"
        android:layout_alignRight="@+id/VoltageBar"
        android:layout_alignStart="@+id/VoltageBar"
        android:layout_below="@+id/VoltageBar"/>

	<view
        android:id="@+id/ampsGraph"
        class="com.github.mikephil.charting.charts.LineChart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/voltsGraph"
        android:layout_alignEnd="@+id/CurrentBar"
        android:layout_alignLeft="@+id/CurrentBar"
        android:layout_alignRight="@+id/CurrentBar"
        android:layout_alignStart="@+id/CurrentBar"
        android:layout_alignTop="@+id/voltsGraph"/>

	<view
        android:id="@+id/RPMGraph"
        class="com.github.mikephil.charting.charts.LineChart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ampsGraph"
        android:layout_alignLeft="@+id/RPMBar"
        android:layout_alignRight="@+id/RPMBar"
        android:layout_alignStart="@+id/RPMBar"
        android:layout_alignTop="@+id/ampsGraph"
        android:layout_toEndOf="@+id/RPMBar"/>

	<view
        android:id="@+id/SpeedGraph"
        class="com.github.mikephil.charting.charts.LineChart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/RPMGraph"
        android:layout_alignEnd="@+id/textView7"
        android:layout_alignLeft="@+id/SpeedBar"
        android:layout_alignParentBottom="false"
        android:layout_alignRight="@+id/SpeedBar"
        android:layout_alignTop="@+id/RPMGraph"
        android:layout_toEndOf="@+id/RPMGraph"/>

	<TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/voltage"
        android:layout_alignLeft="@+id/voltage"
        android:layout_alignRight="@+id/voltage"
        android:gravity="center"
        android:text="Voltage"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20sp"/>

	<TextView
        android:id="@+id/voltage"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/field_height"
        android:layout_below="@+id/textView2"
        android:width="150dp"
        android:gravity="center"
        android:inputType="none"
        android:singleLine="false"
        android:text="##.##"
        android:textSize="@dimen/view4_field_text_size"
        android:textStyle="bold"/>

	<com.ben.drivenbluetooth.util.DataBar
        android:id="@+id/VoltageBar"
        class="com.ben.drivenbluetooth.util.DataBar"
        android:layout_width="@dimen/view4_field_width"
        android:layout_height="@dimen/field_height"
        android:layout_alignLeft="@+id/voltage"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignRight="@+id/voltage"
        android:layout_below="@+id/current"
        custom:BarColor="#A3C2FF"
        custom:max="28"
        custom:min="0"
        custom:xWidth="@integer/databar_xWidth"/>

	<TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignEnd="@+id/current"
        android:layout_alignLeft="@+id/current"
        android:layout_alignRight="@+id/current"
        android:layout_alignTop="@+id/textView2"
        android:gravity="center"
        android:text="Current"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20sp"/>

	<TextView
        android:id="@+id/current"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/field_height"
        android:layout_alignBottom="@+id/voltage"
        android:layout_alignTop="@+id/voltage"
        android:layout_toRightOf="@+id/voltage"
        android:width="150dp"
        android:gravity="center"
        android:inputType="none"
        android:singleLine="false"
        android:text="##.##"
        android:textSize="@dimen/view4_field_text_size"
        android:textStyle="bold"/>

	<com.ben.drivenbluetooth.util.DataBar
        android:id="@+id/CurrentBar"
        class="com.ben.drivenbluetooth.util.DataBar"
        android:layout_width="@dimen/view4_field_width"
        android:layout_height="@dimen/field_height"
        android:layout_alignBottom="@+id/VoltageBar"
        android:layout_alignLeft="@+id/current"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignRight="@+id/current"
        android:layout_below="@+id/current"
        custom:BarColor="#ffffea2e"
        custom:max="50"
        custom:min="0"
        custom:xWidth="@integer/databar_xWidth"/>

	<TextView
        android:id="@+id/rpm"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/field_height"
        android:layout_above="@+id/RPMBar"
        android:layout_alignTop="@+id/current"
        android:layout_toRightOf="@+id/current"
        android:width="150dp"
        android:gravity="center"
        android:inputType="none"
        android:singleLine="false"
        android:text="####"
        android:textSize="@dimen/view4_field_text_size"
        android:textStyle="bold"/>

	<TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/current"
        android:layout_alignEnd="@+id/rpm"
        android:layout_alignLeft="@+id/rpm"
        android:layout_alignRight="@+id/rpm"
        android:layout_alignTop="@+id/textView3"
        android:gravity="center"
        android:text="RPM"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20sp"/>

	<com.ben.drivenbluetooth.util.DataBar
        android:id="@+id/RPMBar"
        class="com.ben.drivenbluetooth.util.DataBar"
        android:layout_width="@dimen/view4_field_width"
        android:layout_height="@dimen/field_height"
        android:layout_alignBottom="@+id/CurrentBar"
        android:layout_alignLeft="@+id/rpm"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignRight="@+id/rpm"
        android:layout_below="@+id/current"
        custom:BarColor="#ffff5358"
        custom:max="2500"
        custom:min="0"
        custom:xWidth="@integer/databar_xWidth"/>

	<TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignEnd="@+id/speed"
        android:layout_alignParentStart="false"
        android:layout_alignTop="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:gravity="center"
        android:text="Speed"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="20sp"/>

	<TextView
        android:id="@+id/speed"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/field_height"
        android:layout_above="@+id/SpeedBar"
        android:layout_alignTop="@+id/rpm"
        android:layout_toRightOf="@+id/rpm"
        android:width="150dp"
        android:gravity="center"
        android:inputType="none"
        android:singleLine="false"
        android:text="#.## mph"
        android:textSize="32sp"
        android:textStyle="bold"/>

	<com.ben.drivenbluetooth.util.DataBar
        android:id="@+id/SpeedBar"
        class="com.ben.drivenbluetooth.util.DataBar"
        android:layout_width="@dimen/view4_field_width"
        android:layout_height="@dimen/field_height"
        android:layout_alignBottom="@+id/RPMBar"
        android:layout_alignLeft="@+id/speed"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignRight="@+id/speed"
        android:layout_below="@+id/current"
        custom:BarColor="#ffff5358"
        custom:max="50"
        custom:min="0"
        custom:xWidth="@integer/databar_xWidth"/>

	<TextView
        android:id="@+id/ampHours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/ampsGraph"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:gravity="center"
        android:text="##.## Ah"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/wattHours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/RPMGraph"
        android:layout_toEndOf="@+id/ampHours"
        android:gravity="center"
        android:text="##.## Wh/km"
        android:textSize="20sp"
        android:textStyle="bold"/>
</RelativeLayout>