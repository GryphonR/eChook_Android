<RelativeLayout
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:layout_below="@+id/textView8"
	android:layout_above="@+id/open"
	android:layout_alignEnd="@+id/txt_Mode"
	android:gravity="center"
	android:id="@+id/CenterView"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:custom="http://schemas.android.com/apk/res-auto">

	<com.github.mikephil.charting.charts.LineChart
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/throttleGraph"
		android:layout_below="@+id/ThrottleBar"
		android:layout_alignParentStart="true"
		android:layout_toStartOf="@+id/VoltageBar"
		android:layout_above="@+id/ampHours"/>

	<view
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.github.mikephil.charting.charts.LineChart"
		android:id="@+id/voltsGraph"
		android:layout_below="@+id/VoltageBar"
		android:layout_alignStart="@+id/VoltageBar"
		android:layout_alignEnd="@+id/VoltageBar"
		android:layout_alignBottom="@+id/throttleGraph"/>

	<view
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.github.mikephil.charting.charts.LineChart"
		android:id="@+id/ampsGraph"
		android:layout_below="@+id/CurrentBar"
		android:layout_alignStart="@+id/CurrentBar"
		android:layout_alignBottom="@+id/voltsGraph"
		android:layout_alignEnd="@+id/CurrentBar"/>

	<view
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.github.mikephil.charting.charts.LineChart"
		android:id="@+id/T1Graph"
		android:layout_below="@+id/T1Bar"
		android:layout_alignStart="@+id/T1Bar"
		android:layout_alignBottom="@+id/ampsGraph"
		android:layout_alignRight="@+id/T1Bar"
		android:layout_toEndOf="@+id/T1Bar"
		android:layout_alignLeft="@+id/T1Bar"/>

	<view
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.github.mikephil.charting.charts.LineChart"
		android:id="@+id/RPMGraph"
		android:layout_below="@+id/T1Bar"
		android:layout_alignStart="@+id/RPMBar"
		android:layout_toEndOf="@+id/RPMBar"
		android:layout_alignBottom="@+id/T1Graph"
		android:layout_alignRight="@+id/RPMBar"
		android:layout_alignLeft="@+id/RPMBar"/>

	<view
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.github.mikephil.charting.charts.LineChart"
		android:id="@+id/SpeedGraph"
		android:layout_below="@+id/RPMBar"
		android:layout_toEndOf="@+id/RPMBar"
		android:layout_alignRight="@+id/SpeedBar"
		android:layout_alignBottom="@+id/throttleGraph"
		android:layout_alignLeft="@+id/SpeedBar"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Throttle"
		android:id="@+id/textView"
		android:gravity="center"
		android:editable="false"
		android:textSize="20sp"
		android:layout_alignParentTop="true"
		android:layout_alignParentStart="true"
		/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="30dp"
		android:id="@+id/throttle"
		android:inputType="none"
		android:width="90dp"
		android:gravity="center"
		android:editable="false"
		android:textSize="20sp"
		android:textStyle="bold"
		android:layout_below="@+id/textView"
		android:layout_alignParentStart="true"/>

	<com.ben.drivenbluetooth.util.DataBar
		android:layout_width="wrap_content"
		android:layout_height="100dp"
		android:id="@+id/ThrottleBar"
		android:layout_below="@+id/current"
		custom:BarColor="#000000"
		custom:min="0"
		custom:max="100"
		custom:xWidth="@integer/databar_xWidth"
		android:layout_alignStart="@+id/throttleGraph"
		android:layout_toStartOf="@+id/VoltageBar"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Voltage"
		android:gravity="center"
		android:id="@+id/textView2"
		android:layout_alignTop="@+id/textView"
		android:layout_alignLeft="@+id/voltage"
		android:layout_alignRight="@+id/voltage"
		android:layout_alignBottom="@+id/textView"
		android:textSize="20sp"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="30dp"
		android:id="@+id/voltage"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="90dp"
		android:layout_alignTop="@+id/throttle"
		android:layout_toRightOf="@+id/throttle"
		android:textSize="20sp"
		android:layout_alignBottom="@+id/throttle"
		android:textStyle="bold"
		/>

	<com.ben.drivenbluetooth.util.DataBar
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.ben.drivenbluetooth.util.DataBar"
		android:id="@+id/VoltageBar"
		android:layout_alignLeft="@+id/voltage"
		android:layout_alignBottom="@+id/ThrottleBar"
		android:layout_alignRight="@+id/voltage"
		android:layout_alignParentStart="false"
		android:layout_alignParentLeft="false"
		android:layout_alignTop="@+id/ThrottleBar"
		custom:BarColor="#A3C2FF"
		custom:min="0"
		custom:max="28"
		custom:xWidth="@integer/databar_xWidth"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Current"
		android:gravity="center"
		android:id="@+id/textView3"
		android:layout_alignTop="@+id/textView2"
		android:layout_alignRight="@+id/current"
		android:layout_alignLeft="@+id/current"
		android:layout_alignBottom="@+id/textView2"
		android:textSize="20sp"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="30dp"
		android:id="@+id/current"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="90dp"
		android:layout_alignTop="@+id/voltage"
		android:layout_toRightOf="@+id/voltage"
		android:textSize="20sp"
		android:layout_alignBottom="@+id/voltage"
		android:textStyle="bold"
		/>

	<com.ben.drivenbluetooth.util.DataBar
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.ben.drivenbluetooth.util.DataBar"
		android:id="@+id/CurrentBar"
		android:layout_alignLeft="@+id/current"
		android:layout_alignBottom="@+id/ThrottleBar"
		android:layout_alignRight="@+id/current"
		android:layout_alignParentStart="false"
		android:layout_alignParentLeft="false"
		android:layout_alignTop="@+id/ThrottleBar"
		custom:BarColor="#ffffea2e"
		custom:min="0"
		custom:max="50"
		custom:xWidth="@integer/databar_xWidth"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Temp 1"
		android:gravity="center"
		android:id="@+id/textView4"
		android:layout_alignBottom="@+id/textView5"
		android:layout_alignTop="@+id/textView7"
		android:textSize="20sp"
		android:layout_toLeftOf="@+id/textView5"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="30dp"
		android:id="@+id/temp1"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="90dp"
		android:layout_alignTop="@+id/current"
		android:textSize="20sp"
		android:layout_above="@+id/T1Bar"
		android:textStyle="bold"
		android:layout_toLeftOf="@+id/rpm"/>

	<com.ben.drivenbluetooth.util.DataBar
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.ben.drivenbluetooth.util.DataBar"
		android:id="@+id/T1Bar"
		android:layout_alignLeft="@+id/temp1"
		android:layout_alignBottom="@+id/ThrottleBar"
		android:layout_alignRight="@+id/temp1"
		android:layout_alignParentStart="false"
		android:layout_alignParentLeft="false"
		android:layout_alignTop="@+id/ThrottleBar"
		custom:BarColor="#ffff5358"
		custom:min="0"
		custom:max="85"
		custom:xWidth="@integer/databar_xWidth"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="30dp"
		android:id="@+id/rpm"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="90dp"
		android:layout_alignTop="@+id/speed"
		android:textSize="20sp"
		android:textStyle="bold"
		android:layout_alignBottom="@+id/speed"
		android:layout_toLeftOf="@+id/speed"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="RPM"
		android:gravity="center"
		android:id="@+id/textView5"
		android:layout_alignTop="@+id/textView7"
		android:layout_alignBottom="@+id/textView7"
		android:textSize="20sp"
		android:layout_toLeftOf="@+id/textView7"/>

	<com.ben.drivenbluetooth.util.DataBar
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.ben.drivenbluetooth.util.DataBar"
		android:id="@+id/RPMBar"
		android:layout_alignLeft="@+id/rpm"
		android:layout_alignBottom="@+id/ThrottleBar"
		android:layout_alignRight="@+id/rpm"
		android:layout_alignParentStart="false"
		android:layout_alignParentLeft="false"
		android:layout_alignTop="@+id/ThrottleBar"
		custom:BarColor="#ffff5358"
		custom:min="0"
		custom:max="2500"
		custom:xWidth="@integer/databar_xWidth"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:text="Speed"
		android:gravity="center"
		android:id="@+id/textView7"
		android:textSize="20sp"
		android:layout_alignParentRight="true"
		android:layout_alignBottom="@+id/textView"
		android:layout_alignTop="@+id/textView"
		android:layout_alignParentTop="true"/>

	<TextView
		android:layout_width="@dimen/six_view_field_width"
		android:layout_height="30dp"
		android:id="@+id/speed"
		android:gravity="center"
		android:singleLine="false"
		android:inputType="none"
		android:width="90dp"
		android:textSize="20sp"
		android:textStyle="bold"
		android:layout_alignRight="@+id/textView7"
		android:layout_below="@+id/textView7"
		android:layout_alignLeft="@+id/textView7"/>

	<com.ben.drivenbluetooth.util.DataBar
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		class="com.ben.drivenbluetooth.util.DataBar"
		android:id="@+id/SpeedBar"
		android:layout_alignLeft="@+id/speed"
		android:layout_alignBottom="@+id/ThrottleBar"
		android:layout_alignRight="@+id/speed"
		android:layout_alignParentStart="false"
		android:layout_alignParentLeft="false"
		android:layout_alignTop="@+id/ThrottleBar"
		custom:BarColor="#ffff5358"
		custom:min="0"
		custom:max="50"
		custom:xWidth="@integer/databar_xWidth"/>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/ampHours"
		android:layout_alignParentBottom="true"
		android:layout_alignParentStart="true"
		android:layout_alignParentEnd="true"
		android:gravity="center"
		android:textStyle="bold"
		android:textSize="20sp"/>

</RelativeLayout>